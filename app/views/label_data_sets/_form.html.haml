= form_for @label_data_set, :html => {:multipart => true} do |f|
  -if @label_data_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@label_data_set.errors.count, "error")} prohibited this label_data_set from being saved:"
      %ul
        - @label_data_set.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
    = f.label :excel_data
    = f.file_field :excel_data
  .actions
    = f.submit 'Save'
